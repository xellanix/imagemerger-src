                              VerticalAlignment="Stretch" IsSwipeEnabled="False"
                              SelectionChanged="ImageSelectionChanged" DragOver="FilesDragOver" Drop="FilesDrop">
                                <ListView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <ItemsStackPanel/>
                                    </ItemsPanelTemplate>
                                </ListView.ItemsPanel>
                                <ListView.ItemContainerTransitions>
                                    <TransitionCollection>
                                        <AddDeleteThemeTransition/>
                                    </TransitionCollection>
                                </ListView.ItemContainerTransitions>
                                <ListView.ItemTemplate>
                                    <DataTemplate x:DataType="local:ImageItem">
                                        <ListViewItem Margin="0">
                                            <Grid ColumnSpacing="12" Margin="0,10,10,10">
