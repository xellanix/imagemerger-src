                                                </Viewbox>
                                                <StackPanel Spacing="5" Grid.Column="1">
                                                    <TextBlock Text="{x:Bind ItemName}" FontWeight="SemiBold" TextWrapping="Wrap" Foreground="{ThemeResource DefaultTextForegroundThemeBrush}"/>
                                                    <TextBlock Text="{x:Bind ItemPath}" FontSize="12" Foreground="{ThemeResource TextFillColorSecondaryBrush}" TextWrapping="Wrap"/>
                                                </StackPanel>
                                            </Grid>
                                        </ListViewItem>
                                    </DataTemplate>
                                </ListView.ItemTemplate>
                            </ListView>
                            <Viewbox x:Name="NoFilePanel" Visibility="Collapsed" Stretch="Uniform" IsHitTestVisible="False" MaxHeight="{Binding ActualHeight, ElementName=ImageList}" HorizontalAlignment="Center" VerticalAlignment="Center">
